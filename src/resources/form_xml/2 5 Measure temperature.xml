<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>2.5 Measure temperature</h:title>
    <model>
      <instance>
        <session_5_chap_2_en id="five_session_second_chapter_en">
          <formhub>
            <uuid/>
          </formhub>
          <chapter1/>
          <s5_q1/>
          <cal1/>
          <s5_q2/>
          <cal2/>
          <s5_q3/>
          <cal3/>
          <correctAns/>
          <cor1/>
          <cor2/>
          <cor3/>
          <cor0/>
          <meta>
            <instanceID/>
          </meta>
        </session_5_chap_2_en>
      </instance>
      <bind nodeset="/session_5_chap_2_en/chapter1" type="select1"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_5_chap_2_en/s5_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_5_chap_2_en/s5_q1  = 't',1,0)" nodeset="/session_5_chap_2_en/cal1" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_5_chap_2_en/s5_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_5_chap_2_en/s5_q2  = 't',1,0)" nodeset="/session_5_chap_2_en/cal2" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_5_chap_2_en/s5_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_5_chap_2_en/s5_q3  = 't',1,0)" nodeset="/session_5_chap_2_en/cal3" type="string"/>
      <bind calculate=" /session_5_chap_2_en/cal1 + /session_5_chap_2_en/cal2 + /session_5_chap_2_en/cal3 " nodeset="/session_5_chap_2_en/correctAns" type="string"/>
      <bind nodeset="/session_5_chap_2_en/cor1" readonly="true()" relevant=" /session_5_chap_2_en/correctAns  = 1" type="string"/>
      <bind nodeset="/session_5_chap_2_en/cor2" readonly="true()" relevant=" /session_5_chap_2_en/correctAns  = 2" type="string"/>
      <bind nodeset="/session_5_chap_2_en/cor3" readonly="true()" relevant=" /session_5_chap_2_en/correctAns  = 3" type="string"/>
      <bind nodeset="/session_5_chap_2_en/cor0" readonly="true()" relevant=" /session_5_chap_2_en/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_5_chap_2_en/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'326498cef8734d9e943c79a2e9755387'" nodeset="/session_5_chap_2_en/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 appearance="html" ref="/session_5_chap_2_en/chapter1">
      <label>Chapter 2</label>
      <item>
        <label>session_five.html</label>
        <value>h</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_5_chap_2_en/s5_q1">
      <label>1. What is the Importance of measuring temperature after birth?</label>
      <item>
        <label>To initiate management of hypo or hyperthermia</label>
        <value>ff</value>
      </item>
      <item>
        <label>To identify wreathe the baby is healthy or not</label>
        <value>f</value>
      </item>
      <item>
        <label>All of above</label>
        <value>t</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_5_chap_2_en/s5_q2">
      <label>2. How long thermometer should be placed in the axilla?</label>
      <item>
        <label>3 minutes</label>
        <value>t</value>
      </item>
      <item>
        <label>2 minutes</label>
        <value>ff</value>
      </item>
      <item>
        <label>1 minute</label>
        <value>f</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_5_chap_2_en/s5_q3">
      <label>3. What does it indicate if baby’s feet are cold and abdomen is warm?</label>
      <item>
        <label>Hyperthermia</label>
        <value>f</value>
      </item>
      <item>
        <label>Cold stress</label>
        <value>t</value>
      </item>
      <item>
        <label>Hypothermia</label>
        <value>ff</value>
      </item>
    </select1>
    <input ref="/session_5_chap_2_en/cor1">
      <label>Your Score 
<output value=" /session_5_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_5_chap_2_en/cor2">
      <label>Your Score 
<output value=" /session_5_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_5_chap_2_en/cor3">
      <label>Your Score 
<output value=" /session_5_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_5_chap_2_en/cor0">
      <label>Your Score 
<output value=" /session_5_chap_2_en/correctAns "/>/3</label>
    </input>
  </h:body>
</h:html>
