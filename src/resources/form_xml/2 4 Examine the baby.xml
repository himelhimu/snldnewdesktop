<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>2.4 Examine the baby</h:title>
    <model>
      <instance>
        <session_4_chap_2_en id="four_session_second_chapter_en">
          <formhub>
            <uuid/>
          </formhub>
          <chapter1/>
          <s4_q1/>
          <cal1/>
          <s4_q2/>
          <cal2/>
          <s4_q3/>
          <cal3/>
          <correctAns/>
          <cor1/>
          <cor2/>
          <cor3/>
          <cor0/>
          <meta>
            <instanceID/>
          </meta>
        </session_4_chap_2_en>
      </instance>
      <bind nodeset="/session_4_chap_2_en/chapter1" type="select1"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_4_chap_2_en/s4_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_4_chap_2_en/s4_q1  = 't',1,0)" nodeset="/session_4_chap_2_en/cal1" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_4_chap_2_en/s4_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_4_chap_2_en/s4_q2  = 't',1,0)" nodeset="/session_4_chap_2_en/cal2" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_4_chap_2_en/s4_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_4_chap_2_en/s4_q3  = 't',1,0)" nodeset="/session_4_chap_2_en/cal3" type="string"/>
      <bind calculate=" /session_4_chap_2_en/cal1 + /session_4_chap_2_en/cal2 + /session_4_chap_2_en/cal3 " nodeset="/session_4_chap_2_en/correctAns" type="string"/>
      <bind nodeset="/session_4_chap_2_en/cor1" readonly="true()" relevant=" /session_4_chap_2_en/correctAns  = 1" type="string"/>
      <bind nodeset="/session_4_chap_2_en/cor2" readonly="true()" relevant=" /session_4_chap_2_en/correctAns  = 2" type="string"/>
      <bind nodeset="/session_4_chap_2_en/cor3" readonly="true()" relevant=" /session_4_chap_2_en/correctAns  = 3" type="string"/>
      <bind nodeset="/session_4_chap_2_en/cor0" readonly="true()" relevant=" /session_4_chap_2_en/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_4_chap_2_en/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'dab6b5e8871b4c88998d96a6abbe8854'" nodeset="/session_4_chap_2_en/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 appearance="html" ref="/session_4_chap_2_en/chapter1">
      <label>Chapter 2</label>
      <item>
        <label>session_four.html</label>
        <value>h</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_4_chap_2_en/s4_q1">
      <label>1. What should you examine of a new born baby within 90 minutes after birth?</label>
      <item>
        <label>Breathing, temperature, feeding, weight, cord stump, skin color, body movement and other anomaly</label>
        <value>t</value>
      </item>
      <item>
        <label>Feeding, weight, cord stump, skin color, body movement and other anomaly</label>
        <value>f</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_4_chap_2_en/s4_q2">
      <label>2. What is the normal respiratory rate and temperature of a baby?</label>
      <item>
        <label>&gt; 60 breaths/ min and 31.50C-32.50C</label>
        <value>f</value>
      </item>
      <item>
        <label>30-59 breaths/min and 36.50C-37.50C</label>
        <value>t</value>
      </item>
      <item>
        <label>&lt; 30 breaths/min and &gt; 380C</label>
        <value>ff</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_4_chap_2_en/s4_q3">
      <label>3. Which one is the correct statement?</label>
      <item>
        <label>Provider should record the findings of examination &amp; share with mother</label>
        <value>t</value>
      </item>
      <item>
        <label>Do not kept the baby worm during examination</label>
        <value>f</value>
      </item>
      <item>
        <label>No need to monitor breathing every 15 minutes after birth</label>
        <value>ff</value>
      </item>
    </select1>
    <input ref="/session_4_chap_2_en/cor1">
      <label>Your Score 
<output value=" /session_4_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_4_chap_2_en/cor2">
      <label>Your Score 
<output value=" /session_4_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_4_chap_2_en/cor3">
      <label>Your Score 
<output value=" /session_4_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_4_chap_2_en/cor0">
      <label>Your Score 
<output value=" /session_4_chap_2_en/correctAns "/>/3</label>
    </input>
  </h:body>
</h:html>
