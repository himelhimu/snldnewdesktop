<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>2.13 Improve thermal care for abnormal temperature</h:title>
    <model>
      <instance>
        <session_13_chap_2_en id="thirteen_session_second_chapter_en">
          <formhub>
            <uuid/>
          </formhub>
          <chapter1/>
          <s13_q1/>
          <cal1/>
          <s13_q2/>
          <cal2/>
          <s13_q3/>
          <cal3/>
          <correctAns/>
          <cor1/>
          <cor2/>
          <cor3/>
          <cor0/>
          <meta>
            <instanceID/>
          </meta>
        </session_13_chap_2_en>
      </instance>
      <bind nodeset="/session_13_chap_2_en/chapter1" type="select1"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_13_chap_2_en/s13_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_13_chap_2_en/s13_q1  = 't',1,0)" nodeset="/session_13_chap_2_en/cal1" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_13_chap_2_en/s13_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_13_chap_2_en/s13_q2  = 't',1,0)" nodeset="/session_13_chap_2_en/cal2" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_13_chap_2_en/s13_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_13_chap_2_en/s13_q3  = 't',1,0)" nodeset="/session_13_chap_2_en/cal3" type="string"/>
      <bind calculate=" /session_13_chap_2_en/cal1 + /session_13_chap_2_en/cal2 + /session_13_chap_2_en/cal3 " nodeset="/session_13_chap_2_en/correctAns" type="string"/>
      <bind nodeset="/session_13_chap_2_en/cor1" readonly="true()" relevant=" /session_13_chap_2_en/correctAns  = 1" type="string"/>
      <bind nodeset="/session_13_chap_2_en/cor2" readonly="true()" relevant=" /session_13_chap_2_en/correctAns  = 2" type="string"/>
      <bind nodeset="/session_13_chap_2_en/cor3" readonly="true()" relevant=" /session_13_chap_2_en/correctAns  = 3" type="string"/>
      <bind nodeset="/session_13_chap_2_en/cor0" readonly="true()" relevant=" /session_13_chap_2_en/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_13_chap_2_en/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'265229ae0bd74ba3b184b29d6a62a341'" nodeset="/session_13_chap_2_en/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 appearance="html" ref="/session_13_chap_2_en/chapter1">
      <label>Chapter 2</label>
      <item>
        <label>session_thirteen.html</label>
        <value>h</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_13_chap_2_en/s13_q1">
      <label>1. What should you do if temperature of newborn is below normal?</label>
      <item>
        <label>Keep the room warm</label>
        <value>ff</value>
      </item>
      <item>
        <label>Replace wet choths</label>
        <value>f</value>
      </item>
      <item>
        <label>All of above</label>
        <value>t</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_13_chap_2_en/s13_q2">
      <label>2. How long you monitor the temperature when it is billow 35.50C/95.90F?</label>
      <item>
        <label>Hourly till temperature became normal</label>
        <value>t</value>
      </item>
      <item>
        <label>2 hourly till temperature became normal</label>
        <value>ff</value>
      </item>
      <item>
        <label>None of above</label>
        <value>ff</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_13_chap_2_en/s13_q3">
      <label>3. Which statement is correct?</label>
      <item>
        <label>Temperature is &gt; 37.50C/ 99.50F a danger sign</label>
        <value>f</value>
      </item>
      <item>
        <label>To improve thermal care continue skin to skin care, warped with dry cloth, cover the head properly</label>
        <value>ff</value>
      </item>
      <item>
        <label>Both of above</label>
        <value>t</value>
      </item>
    </select1>
    <input ref="/session_13_chap_2_en/cor1">
      <label>Your Score 
<output value=" /session_13_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_13_chap_2_en/cor2">
      <label>Your Score 
<output value=" /session_13_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_13_chap_2_en/cor3">
      <label>Your Score 
<output value=" /session_13_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_13_chap_2_en/cor0">
      <label>Your Score 
<output value=" /session_13_chap_2_en/correctAns "/>/3</label>
    </input>
  </h:body>
</h:html>
