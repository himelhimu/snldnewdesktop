<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>2.10 For a healthy baby, maintain normal temperature</h:title>
    <model>
      <instance>
        <session_10_chap_2_en id="ten_session_second_chapter_en">
          <formhub>
            <uuid/>
          </formhub>
          <chapter1/>
          <s10_q1/>
          <cal1/>
          <s10_q2/>
          <cal2/>
          <s10_q3/>
          <cal3/>
          <correctAns/>
          <cor1/>
          <cor2/>
          <cor3/>
          <cor0/>
          <meta>
            <instanceID/>
          </meta>
        </session_10_chap_2_en>
      </instance>
      <bind nodeset="/session_10_chap_2_en/chapter1" type="select1"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_10_chap_2_en/s10_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_10_chap_2_en/s10_q1  = 't',1,0)" nodeset="/session_10_chap_2_en/cal1" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_10_chap_2_en/s10_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_10_chap_2_en/s10_q2  = 't',1,0)" nodeset="/session_10_chap_2_en/cal2" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_10_chap_2_en/s10_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_10_chap_2_en/s10_q3  = 't',1,0)" nodeset="/session_10_chap_2_en/cal3" type="string"/>
      <bind calculate=" /session_10_chap_2_en/cal1 + /session_10_chap_2_en/cal2 + /session_10_chap_2_en/cal3 " nodeset="/session_10_chap_2_en/correctAns" type="string"/>
      <bind nodeset="/session_10_chap_2_en/cor1" readonly="true()" relevant=" /session_10_chap_2_en/correctAns  = 1" type="string"/>
      <bind nodeset="/session_10_chap_2_en/cor2" readonly="true()" relevant=" /session_10_chap_2_en/correctAns  = 2" type="string"/>
      <bind nodeset="/session_10_chap_2_en/cor3" readonly="true()" relevant=" /session_10_chap_2_en/correctAns  = 3" type="string"/>
      <bind nodeset="/session_10_chap_2_en/cor0" readonly="true()" relevant=" /session_10_chap_2_en/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_10_chap_2_en/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'d1b0b326fc724e8fb13107dda2594f58'" nodeset="/session_10_chap_2_en/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 appearance="html" ref="/session_10_chap_2_en/chapter1">
      <label>Chapter 2</label>
      <item>
        <label>session_ten.html</label>
        <value>h</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_10_chap_2_en/s10_q1">
      <label>1. How soon does a baby begin to lose heat?</label>
      <item>
        <label>When the baby starts breastfeeding</label>
        <value>f</value>
      </item>
      <item>
        <label>Immediately after birth, even before the body temperature begins to fall</label>
        <value>t</value>
      </item>
      <item>
        <label>After 90 minutes</label>
        <value>ff</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_10_chap_2_en/s10_q2">
      <label>2. How long should skin-to-skin care last for all babies?</label>
      <item>
        <label>At least 2 hours after birth</label>
        <value>t</value>
      </item>
      <item>
        <label>As long as it is convenient for the mother</label>
        <value>ff</value>
      </item>
      <item>
        <label>At least 24 hours after birth</label>
        <value>f</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_10_chap_2_en/s10_q3">
      <label>3. How should a baby be kept warm after skin-to-skin care?</label>
      <item>
        <label>Bathing in warm water</label>
        <value>f</value>
      </item>
      <item>
        <label>Wrapping in a clean, dry blanket or cloth</label>
        <value>t</value>
      </item>
      <item>
        <label>Exposing the baby to direct sunlight</label>
        <value>ff</value>
      </item>
    </select1>
    <input ref="/session_10_chap_2_en/cor1">
      <label>Your Score 
<output value=" /session_10_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_10_chap_2_en/cor2">
      <label>Your Score 
<output value=" /session_10_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_10_chap_2_en/cor3">
      <label>Your Score 
<output value=" /session_10_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_10_chap_2_en/cor0">
      <label>Your Score 
<output value=" /session_10_chap_2_en/correctAns "/>/3</label>
    </input>
  </h:body>
</h:html>
