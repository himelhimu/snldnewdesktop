<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>২.১৫ মায়ের দুধ সরাসরি খেতে না পারলে শিশুকে বুকের দুধ গেলে খাওয়ান</h:title>
    <model>
      <instance>
        <session_15_chap_2 id="fifteen_session_second_chapter">
          <formhub>
            <uuid/>
          </formhub>
          <chapter2/>
          <s15_q1/>
          <cal1/>
          <s15_q2/>
          <cal2/>
          <s15_q3/>
          <cal3/>
          <correctAns/>
          <trans/>
          <transs/>
          <transss/>
          <transsss/>
          <corrAns1/>
          <corrAns2/>
          <corrAns3/>
          <corrAns4/>
          <meta>
            <instanceID/>
          </meta>
        </session_15_chap_2>
      </instance>
      <bind nodeset="/session_15_chap_2/chapter2" type="select1"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_15_chap_2/s15_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_15_chap_2/s15_q1  = 't',1,0)" nodeset="/session_15_chap_2/cal1" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_15_chap_2/s15_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_15_chap_2/s15_q2  = 't',1,0)" nodeset="/session_15_chap_2/cal2" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_15_chap_2/s15_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_15_chap_2/s15_q3  = 't',1,0)" nodeset="/session_15_chap_2/cal3" type="string"/>
      <bind calculate=" /session_15_chap_2/cal1 + /session_15_chap_2/cal2 + /session_15_chap_2/cal3 " nodeset="/session_15_chap_2/correctAns" type="string"/>
      <bind calculate="if( /session_15_chap_2/cal1 + /session_15_chap_2/cal2 + /session_15_chap_2/cal3 =1,'১',' ')" nodeset="/session_15_chap_2/trans" type="string"/>
      <bind calculate="if( /session_15_chap_2/cal1 + /session_15_chap_2/cal2 + /session_15_chap_2/cal3 =2,'২',' ')" nodeset="/session_15_chap_2/transs" type="string"/>
      <bind calculate="if( /session_15_chap_2/cal1 + /session_15_chap_2/cal2 + /session_15_chap_2/cal3 =3,'৩',' ')" nodeset="/session_15_chap_2/transss" type="string"/>
      <bind calculate="if( /session_15_chap_2/cal1 + /session_15_chap_2/cal2 + /session_15_chap_2/cal3 =0,'০',' ')" nodeset="/session_15_chap_2/transsss" type="string"/>
      <bind nodeset="/session_15_chap_2/corrAns1" readonly="true()" relevant=" /session_15_chap_2/correctAns  = 1" type="string"/>
      <bind nodeset="/session_15_chap_2/corrAns2" readonly="true()" relevant=" /session_15_chap_2/correctAns  = 2" type="string"/>
      <bind nodeset="/session_15_chap_2/corrAns3" readonly="true()" relevant=" /session_15_chap_2/correctAns  = 3" type="string"/>
      <bind nodeset="/session_15_chap_2/corrAns4" readonly="true()" relevant=" /session_15_chap_2/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_15_chap_2/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'f99a9cef2200403eae7f6acd96d10231'" nodeset="/session_15_chap_2/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 appearance="html" ref="/session_15_chap_2/chapter2">
      <label>chapter 2</label>
      <item>
        <label>session_fifteen.html</label>
        <value>h</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_15_chap_2/s15_q1">
      <label>১.কোন পরিস্থিতিতে দুধ গেলে বের করতে হবে?</label>
      <item>
        <label>যখন শিশু সরাসরি মায়ের দুধ টেনে খেতে পারে না</label>
        <value>f</value>
      </item>
      <item>
        <label>ফোলা স্তনের ব্যবস্থাপনার জন্য</label>
        <value>ff</value>
      </item>
      <item>
        <label>উপরের সবক’টি</label>
        <value>t</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_15_chap_2/s15_q2">
      <label>২.কতক্ষণ দুধ গালাবেন?</label>
      <item>
        <label>এক ঘন্টা পর্যন্ত</label>
        <value>f</value>
      </item>
      <item>
        <label>দুই ঘন্টা পর্যন্ত</label>
        <value>ff</value>
      </item>
      <item>
        <label>দুধের ধারা যতক্ষণ অব্যাহত থাকবে ততক্ষণ পর্যন্ত</label>
        <value>t</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_15_chap_2/s15_q3">
      <label>৩.গালানো দুধ কিভাবে সংরক্ষণ করতে হবে?</label>
      <item>
        <label>ঢাকনাযুক্ত পরিষ্কার পাত্রে</label>
        <value>f</value>
      </item>
      <item>
        <label>ঠান্ডা জায়গায় ৬ ঘন্টা পর্যন্ত</label>
        <value>ff</value>
      </item>
      <item>
        <label>উপরের সবক’টি</label>
        <value>t</value>
      </item>
    </select1>
    <input ref="/session_15_chap_2/corrAns1">
      <label>আপনার স্কোর 

<output value=" /session_15_chap_2/trans "/>/৩</label>
    </input>
    <input ref="/session_15_chap_2/corrAns2">
      <label>আপনার স্কোর 

<output value=" /session_15_chap_2/transs "/>/৩</label>
    </input>
    <input ref="/session_15_chap_2/corrAns3">
      <label>আপনার স্কোর 

<output value=" /session_15_chap_2/transss "/>/৩</label>
    </input>
    <input ref="/session_15_chap_2/corrAns4">
      <label>আপনার স্কোর 

<output value=" /session_15_chap_2/transsss "/>/৩</label>
    </input>
  </h:body>
</h:html>
