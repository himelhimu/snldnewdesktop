<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>২.৩ নাভির পরিচর্যা দিন</h:title>
    <model>
      <instance>
        <session_3_chap_2 id="third_session_second_chapter">
          <formhub>
            <uuid/>
          </formhub>
          <chapter1/>
          <s3_q1/>
          <cal1/>
          <s3_q2/>
          <cal2/>
          <s3_q3/>
          <cal3/>
          <correctAns/>
          <trans/>
          <transs/>
          <transss/>
          <transsss/>
          <corrAns1/>
          <corrAns2/>
          <corrAns3/>
          <corrAns4/>
          <meta>
            <instanceID/>
          </meta>
        </session_3_chap_2>
      </instance>
      <bind nodeset="/session_3_chap_2/chapter1" type="select1"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_3_chap_2/s3_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_3_chap_2/s3_q1  = 't',1,0)" nodeset="/session_3_chap_2/cal1" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_3_chap_2/s3_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_3_chap_2/s3_q2  = 't',1,0)" nodeset="/session_3_chap_2/cal2" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_3_chap_2/s3_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_3_chap_2/s3_q3  = 't',1,0)" nodeset="/session_3_chap_2/cal3" type="string"/>
      <bind calculate=" /session_3_chap_2/cal1 + /session_3_chap_2/cal2 + /session_3_chap_2/cal3 " nodeset="/session_3_chap_2/correctAns" type="string"/>
      <bind calculate="if( /session_3_chap_2/cal1 + /session_3_chap_2/cal2 + /session_3_chap_2/cal3 =1,'১',' ')" nodeset="/session_3_chap_2/trans" type="string"/>
      <bind calculate="if( /session_3_chap_2/cal1 + /session_3_chap_2/cal2 + /session_3_chap_2/cal3 =2,'২',' ')" nodeset="/session_3_chap_2/transs" type="string"/>
      <bind calculate="if( /session_3_chap_2/cal1 + /session_3_chap_2/cal2 + /session_3_chap_2/cal3 =3,'৩',' ')" nodeset="/session_3_chap_2/transss" type="string"/>
      <bind calculate="if( /session_3_chap_2/cal1 + /session_3_chap_2/cal2 + /session_3_chap_2/cal3 =0,'০',' ')" nodeset="/session_3_chap_2/transsss" type="string"/>
      <bind nodeset="/session_3_chap_2/corrAns1" readonly="true()" relevant=" /session_3_chap_2/correctAns  = 1" type="string"/>
      <bind nodeset="/session_3_chap_2/corrAns2" readonly="true()" relevant=" /session_3_chap_2/correctAns  = 2" type="string"/>
      <bind nodeset="/session_3_chap_2/corrAns3" readonly="true()" relevant=" /session_3_chap_2/correctAns  = 3" type="string"/>
      <bind nodeset="/session_3_chap_2/corrAns4" readonly="true()" relevant=" /session_3_chap_2/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_3_chap_2/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'a702b68a707446648c1b994fe3033293'" nodeset="/session_3_chap_2/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 appearance="html" ref="/session_3_chap_2/chapter1">
      <label>Chapter 2</label>
      <item>
        <label>session_three.html</label>
        <value>h</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_3_chap_2/s3_q1">
      <label>১. শিশুর নাভি কাটার পর কি লাগাবেন?</label>
      <item>
        <label>৭.১% ক্লোরহেক্সিডিন দ্রবণ</label>
        <value>t</value>
      </item>
      <item>
        <label>ব্যাণ্ডজে</label>
        <value>f</value>
      </item>
      <item>
        <label>গোবর, সিঁদুর বা সরিষার তৈল</label>
        <value>ff</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_3_chap_2/s3_q2">
      <label>২. কখন ৭.১% ক্লোরহেক্সিডিন ব্যবহার করবেন?</label>
      <item>
        <label>নাড়ী কাটার পরপরই</label>
        <value>t</value>
      </item>
      <item>
        <label>জন্মের ৭২ ঘন্টা পর</label>
        <value>f</value>
      </item>
      <item>
        <label>কখনই না</label>
        <value>ff</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_3_chap_2/s3_q3">
      <label>৩. ৭.১% ক্লোরহেক্সিডিন সরবরাহ কোথা থেকে পাবেন?</label>
      <item>
        <label>উপজেলা স্বাস্থ্য কমপ্লেক্স থেকে</label>
        <value>f</value>
      </item>
      <item>
        <label>সদর হাসপাতাল, মেডিকেল কলেজ হাসপাতাল থেকে</label>
        <value>ff</value>
      </item>
      <item>
        <label>উপরের সবগুলো</label>
        <value>t</value>
      </item>
    </select1>
    <input ref="/session_3_chap_2/corrAns1">
      <label>আপনার স্কোর 

<output value=" /session_3_chap_2/trans "/>/৩</label>
    </input>
    <input ref="/session_3_chap_2/corrAns2">
      <label>আপনার স্কোর 

<output value=" /session_3_chap_2/transs "/>/৩</label>
    </input>
    <input ref="/session_3_chap_2/corrAns3">
      <label>আপনার স্কোর 

<output value=" /session_3_chap_2/transss "/>/৩</label>
    </input>
    <input ref="/session_3_chap_2/corrAns4">
      <label>আপনার স্কোর 

<output value=" /session_3_chap_2/transsss "/>/৩</label>
    </input>
  </h:body>
</h:html>
