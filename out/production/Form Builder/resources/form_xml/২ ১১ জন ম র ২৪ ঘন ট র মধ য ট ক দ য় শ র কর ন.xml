<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>২.১১ জন্মের ২৪ ঘন্টার মধ্যে টিকা দেয়া শুরু করুন</h:title>
    <model>
      <instance>
        <session_11_chap_2 id="eleven_session_second_chapter">
          <formhub>
            <uuid/>
          </formhub>
          <chapter2/>
          <s11_q1/>
          <cal1/>
          <s11_q2/>
          <cal2/>
          <s11_q3/>
          <cal3/>
          <correctAns/>
          <trans/>
          <transs/>
          <transss/>
          <transsss/>
          <corrAns1/>
          <corrAns2/>
          <corrAns3/>
          <corrAns4/>
          <meta>
            <instanceID/>
          </meta>
        </session_11_chap_2>
      </instance>
      <bind nodeset="/session_11_chap_2/chapter2" type="select1"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_11_chap_2/s11_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_11_chap_2/s11_q1  = 't',1,0)" nodeset="/session_11_chap_2/cal1" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_11_chap_2/s11_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_11_chap_2/s11_q2  = 't',1,0)" nodeset="/session_11_chap_2/cal2" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_11_chap_2/s11_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_11_chap_2/s11_q3  = 't',1,0)" nodeset="/session_11_chap_2/cal3" type="string"/>
      <bind calculate=" /session_11_chap_2/cal1 + /session_11_chap_2/cal2 + /session_11_chap_2/cal3 " nodeset="/session_11_chap_2/correctAns" type="string"/>
      <bind calculate="if( /session_11_chap_2/cal1 + /session_11_chap_2/cal2 + /session_11_chap_2/cal3 =1,'১',' ')" nodeset="/session_11_chap_2/trans" type="string"/>
      <bind calculate="if( /session_11_chap_2/cal1 + /session_11_chap_2/cal2 + /session_11_chap_2/cal3 =2,'২',' ')" nodeset="/session_11_chap_2/transs" type="string"/>
      <bind calculate="if( /session_11_chap_2/cal1 + /session_11_chap_2/cal2 + /session_11_chap_2/cal3 =3,'৩',' ')" nodeset="/session_11_chap_2/transss" type="string"/>
      <bind calculate="if( /session_11_chap_2/cal1 + /session_11_chap_2/cal2 + /session_11_chap_2/cal3 =0,'০',' ')" nodeset="/session_11_chap_2/transsss" type="string"/>
      <bind nodeset="/session_11_chap_2/corrAns1" readonly="true()" relevant=" /session_11_chap_2/correctAns  = 1" type="string"/>
      <bind nodeset="/session_11_chap_2/corrAns2" readonly="true()" relevant=" /session_11_chap_2/correctAns  = 2" type="string"/>
      <bind nodeset="/session_11_chap_2/corrAns3" readonly="true()" relevant=" /session_11_chap_2/correctAns  = 3" type="string"/>
      <bind nodeset="/session_11_chap_2/corrAns4" readonly="true()" relevant=" /session_11_chap_2/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_11_chap_2/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'fb87b40deb6d4929bcf1448416e51f31'" nodeset="/session_11_chap_2/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 appearance="html" ref="/session_11_chap_2/chapter2">
      <label>chapter 2</label>
      <item>
        <label>session_eleven.html</label>
        <value>h</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_11_chap_2/s11_q1">
      <label>১. জন্মের পর পর কোন টিকা শিশুকে দিতে হবে?</label>
      <item>
        <label>বিসিজি</label>
        <value>f</value>
      </item>
      <item>
        <label>ওপিভি</label>
        <value>ff</value>
      </item>
      <item>
        <label>উপরের দু’টি</label>
        <value>t</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_11_chap_2/s11_q2">
      <label>২. বিসিজি টিকা দেয়ার স্থানে কি ব্যবহার করতে হবে?</label>
      <item>
        <label>তৈল</label>
        <value>f</value>
      </item>
      <item>
        <label>ঔষধ</label>
        <value>ff</value>
      </item>
      <item>
        <label>কোন কিছু নয়</label>
        <value>t</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_11_chap_2/s11_q3">
      <label>৩. টিকার জন্য ব্যবহৃত সিরিঞ্জ কোথায় ফেলতে হবে?</label>
      <item>
        <label>সাধারণ বর্জ্য ফেলার পাত্রে</label>
        <value>f</value>
      </item>
      <item>
        <label>ধারালো বর্জ্য (ছিদ্ররোধী পাত্রে) ফেলার পাত্রে</label>
        <value>ff</value>
      </item>
      <item>
        <label>উপরের কোনটি’ই নয়</label>
        <value>t</value>
      </item>
    </select1>
    <input ref="/session_11_chap_2/corrAns1">
      <label>আপনার স্কোর 

<output value=" /session_11_chap_2/trans "/>/৩</label>
    </input>
    <input ref="/session_11_chap_2/corrAns2">
      <label>আপনার স্কোর 

<output value=" /session_11_chap_2/transs "/>/৩</label>
    </input>
    <input ref="/session_11_chap_2/corrAns3">
      <label>আপনার স্কোর 

<output value=" /session_11_chap_2/transss "/>/৩</label>
    </input>
    <input ref="/session_11_chap_2/corrAns4">
      <label>আপনার স্কোর 

<output value=" /session_11_chap_2/transsss "/>/৩</label>
    </input>
  </h:body>
</h:html>
