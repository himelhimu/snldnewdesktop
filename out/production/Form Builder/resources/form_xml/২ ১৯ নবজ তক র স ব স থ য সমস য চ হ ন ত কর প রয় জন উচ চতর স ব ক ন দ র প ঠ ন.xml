    <input ref="/session_19_chap_2/corrAns2">
      <label>আপনার স্কোর 

<output value=" /session_19_chap_2/transs "/>/৩</label>
    </input>
    <input ref="/session_19_chap_2/corrAns3">
      <label>আপনার স্কোর 

<output value=" /session_19_chap_2/transss "/>/৩</label>
    </input>
    <input ref="/session_19_chap_2/corrAns4">
      <label>আপনার স্কোর 

<output value=" /session_19_chap_2/transsss "/>/৩</label>
