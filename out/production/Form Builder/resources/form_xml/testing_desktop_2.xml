<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms/" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>TESTING DESKTOP2</h:title>
    <model>
      <instance>
        <testing_desktop_2 id="testing_desktop_2">
          <Enter_your_question/>
          <enter_another_question/>
          <start/>
          <end/>
          <username/>
          <meta>
            <instanceID/>
          </meta>
        </testing_desktop_2>
      </instance>
      <bind nodeset="/testing_desktop_2/Enter_your_question" required="true()" type="string"/>
      <bind nodeset="/testing_desktop_2/enter_another_question" required="true()" type="string"/>
      <bind jr:preload="timestamp" jr:preloadParams="start" nodeset="/testing_desktop_2/start" type="dateTime"/>
      <bind jr:preload="timestamp" jr:preloadParams="end" nodeset="/testing_desktop_2/end" type="dateTime"/>
      <bind jr:preload="property" jr:preloadParams="username" nodeset="/testing_desktop_2/username" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/testing_desktop_2/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/testing_desktop_2/Enter_your_question">
      <label>Enter your question</label>
    </input>
    <input ref="/testing_desktop_2/enter_another_question">
      <label>Enter another question</label>
    </input>
  </h:body>
</h:html>
