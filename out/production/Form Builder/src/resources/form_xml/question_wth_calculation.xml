<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Question_with_calculation</h:title>
    <model>
      <instance>
        <uploaded_form_86zybu id="question_wth_calculation">
          <formhub>
            <uuid/>
          </formhub>
          <one/>
          <calcu1/>
          <two/>
          <calcu2/>
          <three/>
          <calcu3/>
          <cal4/>
          <meta>
            <instanceID/>
          </meta>
        </uploaded_form_86zybu>
      </instance>
      <bind nodeset="/uploaded_form_86zybu/one" type="select1"/>
      <bind calculate="if( /uploaded_form_86zybu/one  = 't',1,0)" nodeset="/uploaded_form_86zybu/calcu1" type="string"/>
      <bind nodeset="/uploaded_form_86zybu/two" type="select1"/>
      <bind calculate="if( /uploaded_form_86zybu/two  = 't',1,0)" nodeset="/uploaded_form_86zybu/calcu2" type="string"/>
      <bind nodeset="/uploaded_form_86zybu/three" type="select1"/>
      <bind calculate="if( /uploaded_form_86zybu/three  = 't',1,0)" nodeset="/uploaded_form_86zybu/calcu3" type="string"/>
      <bind calculate=" /uploaded_form_86zybu/calcu1 + /uploaded_form_86zybu/calcu2 + /uploaded_form_86zybu/calcu3 " nodeset="/uploaded_form_86zybu/cal4" readonly="true()" type="int"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/uploaded_form_86zybu/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'74e15d115bd648ef80e12bacd64f01cc'" nodeset="/uploaded_form_86zybu/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 ref="/uploaded_form_86zybu/one">
      <label>When should a skilled person be present at a birth?</label>
      <item>
        <label>If problems occur</label>
        <value>f</value>
      </item>
      <item>
        <label>At every birth</label>
        <value>t</value>
      </item>
      <item>
        <label>After every birth</label>
        <value>ff</value>
      </item>
    </select1>
    <select1 ref="/uploaded_form_86zybu/two">
      <label>What is the equipment using during safe delivary?</label>
      <item>
        <label>Safe Delivary Kit</label>
        <value>ff</value>
      </item>
      <item>
        <label>Suction device, sthetescope, watch/timer, bag and mask</label>
        <value>f</value>
      </item>
      <item>
        <label>Both of the above (Correct Answer)</label>
        <value>t</value>
      </item>
    </select1>
    <select1 ref="/uploaded_form_86zybu/three">
      <label>What is the correct steps of disinfection of bag, mask and sucker?</label>
      <item>
        <label>By submersion in 0.5% chlorine solution for 10 munites, boiling for 20 munites or by autoclaving (Correct Answer)</label>
        <value>t</value>
      </item>
      <item>
        <label>By submersion in 0.5% chlorine solution for 10 munites only</label>
        <value>f</value>
      </item>
      <item>
        <label>Boiling for  20 munites or by autoclaving only</label>
        <value>ff</value>
      </item>
    </select1>
    <input ref="/uploaded_form_86zybu/cal4">
      <label>Correct Answer</label>
    </input>
  </h:body>
</h:html>
