      <bind nodeset="/session_17_chap_2/chapter2" type="select1"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_17_chap_2/s17_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_17_chap_2/s17_q1  = 't',1,0)" nodeset="/session_17_chap_2/cal1" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_17_chap_2/s17_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_17_chap_2/s17_q2  = 't',1,0)" nodeset="/session_17_chap_2/cal2" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_17_chap_2/s17_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_17_chap_2/s17_q3  = 't',1,0)" nodeset="/session_17_chap_2/cal3" type="string"/>
      <bind calculate=" /session_17_chap_2/cal1 + /session_17_chap_2/cal2 + /session_17_chap_2/cal3 " nodeset="/session_17_chap_2/correctAns" type="string"/>
      <bind calculate="if( /session_17_chap_2/cal1 + /session_17_chap_2/cal2 + /session_17_chap_2/cal3 =1,'১',' ')" nodeset="/session_17_chap_2/trans" type="string"/>
      <bind calculate="if( /session_17_chap_2/cal1 + /session_17_chap_2/cal2 + /session_17_chap_2/cal3 =2,'২',' ')" nodeset="/session_17_chap_2/transs" type="string"/>
      <bind calculate="if( /session_17_chap_2/cal1 + /session_17_chap_2/cal2 + /session_17_chap_2/cal3 =3,'৩',' ')" nodeset="/session_17_chap_2/transss" type="string"/>
      <bind calculate="if( /session_17_chap_2/cal1 + /session_17_chap_2/cal2 + /session_17_chap_2/cal3 =0,'০',' ')" nodeset="/session_17_chap_2/transsss" type="string"/>
      <bind nodeset="/session_17_chap_2/corrAns1" readonly="true()" relevant=" /session_17_chap_2/correctAns  = 1" type="string"/>
      <bind nodeset="/session_17_chap_2/corrAns2" readonly="true()" relevant=" /session_17_chap_2/correctAns  = 2" type="string"/>
      <bind nodeset="/session_17_chap_2/corrAns3" readonly="true()" relevant=" /session_17_chap_2/correctAns  = 3" type="string"/>
      <bind nodeset="/session_17_chap_2/corrAns4" readonly="true()" relevant=" /session_17_chap_2/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_17_chap_2/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'08c3ed608aab4986ae4ba79128f260b7'" nodeset="/session_17_chap_2/formhub/uuid" type="string"/>
    </model>
