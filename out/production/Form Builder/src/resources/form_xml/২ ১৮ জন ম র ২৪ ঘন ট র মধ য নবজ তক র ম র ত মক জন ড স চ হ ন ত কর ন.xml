<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>২.১৮ জন্মের ২৪ ঘন্টার মধ্যে নবজাতকের মারাত্মক জন্ডিস চিহ্নিত করুন</h:title>
    <model>
      <instance>
        <session_18_chap_2 id="eighteen_session_second_chapter">
          <formhub>
            <uuid/>
          </formhub>
          <chapter2/>
          <s18_q1/>
          <cal1/>
          <s18_q2/>
          <cal2/>
          <s18_q3/>
          <cal3/>
          <correctAns/>
          <trans/>
          <transs/>
          <transss/>
          <transsss/>
          <corrAns1/>
          <corrAns2/>
          <corrAns3/>
          <corrAns4/>
          <meta>
            <instanceID/>
          </meta>
        </session_18_chap_2>
      </instance>
      <bind nodeset="/session_18_chap_2/chapter2" type="select1"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_18_chap_2/s18_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_18_chap_2/s18_q1  = 't',1,0)" nodeset="/session_18_chap_2/cal1" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_18_chap_2/s18_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_18_chap_2/s18_q2  = 't',1,0)" nodeset="/session_18_chap_2/cal2" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_18_chap_2/s18_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_18_chap_2/s18_q3  = 't',1,0)" nodeset="/session_18_chap_2/cal3" type="string"/>
      <bind calculate=" /session_18_chap_2/cal1 + /session_18_chap_2/cal2 + /session_18_chap_2/cal3 " nodeset="/session_18_chap_2/correctAns" type="string"/>
      <bind calculate="if( /session_18_chap_2/cal1 + /session_18_chap_2/cal2 + /session_18_chap_2/cal3 =1,'১',' ')" nodeset="/session_18_chap_2/trans" type="string"/>
      <bind calculate="if( /session_18_chap_2/cal1 + /session_18_chap_2/cal2 + /session_18_chap_2/cal3 =2,'২',' ')" nodeset="/session_18_chap_2/transs" type="string"/>
      <bind calculate="if( /session_18_chap_2/cal1 + /session_18_chap_2/cal2 + /session_18_chap_2/cal3 =3,'৩',' ')" nodeset="/session_18_chap_2/transss" type="string"/>
      <bind calculate="if( /session_18_chap_2/cal1 + /session_18_chap_2/cal2 + /session_18_chap_2/cal3 =0,'০',' ')" nodeset="/session_18_chap_2/transsss" type="string"/>
      <bind nodeset="/session_18_chap_2/corrAns1" readonly="true()" relevant=" /session_18_chap_2/correctAns  = 1" type="string"/>
      <bind nodeset="/session_18_chap_2/corrAns2" readonly="true()" relevant=" /session_18_chap_2/correctAns  = 2" type="string"/>
      <bind nodeset="/session_18_chap_2/corrAns3" readonly="true()" relevant=" /session_18_chap_2/correctAns  = 3" type="string"/>
      <bind nodeset="/session_18_chap_2/corrAns4" readonly="true()" relevant=" /session_18_chap_2/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_18_chap_2/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'d2c0b609692741e385f48dc40d85506f'" nodeset="/session_18_chap_2/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 appearance="html" ref="/session_18_chap_2/chapter2">
      <label>chapter 2</label>
      <item>
        <label>session_eighteen.html</label>
        <value>h</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_18_chap_2/s18_q1">
      <label>১.অপরিণত শিশু বা শিশুর সংক্রমণ ছাড়াও মারাত্মক জন্ডিসের জন্য ঝুঁকিপূর্ণ অবস্থা কোনটি?</label>
      <item>
        <label>যে শিশুটির খাওয়ার পরিমাণ অত্যন্ত কম</label>
        <value>t</value>
      </item>
      <item>
        <label>মেয়ে শিশু</label>
        <value>f</value>
      </item>
      <item>
        <label>বয়স্ক মায়েদের শিশু</label>
        <value>ff</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_18_chap_2/s18_q2">
      <label>২.মারাত্মক জন্ডিস শরীরের কোথায় প্রথমে দেখা যায়?</label>
      <item>
        <label>পিঠে</label>
        <value>f</value>
      </item>
      <item>
        <label>পেটে</label>
        <value>ff</value>
      </item>
      <item>
        <label>মুখে</label>
        <value>t</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_18_chap_2/s18_q3">
      <label>৩.জন্মের ২৪ ঘন্টা পর শরীরের কোন অংশে জন্ডিস দেখা দিলে মারাত্মক জন্ডিস হিসাবে চিহ্নিত হবে?</label>
      <item>
        <label>পেটে এবং পিঠে</label>
        <value>f</value>
      </item>
      <item>
        <label>হাতে এবং পায়ে</label>
        <value>ff</value>
      </item>
      <item>
        <label>হাতের তালুতে এবং পায়ের তালুতে</label>
        <value>t</value>
      </item>
    </select1>
    <input ref="/session_18_chap_2/corrAns1">
      <label>আপনার স্কোর 

<output value=" /session_18_chap_2/trans "/>/৩</label>
    </input>
    <input ref="/session_18_chap_2/corrAns2">
      <label>আপনার স্কোর 

<output value=" /session_18_chap_2/transs "/>/৩</label>
    </input>
    <input ref="/session_18_chap_2/corrAns3">
      <label>আপনার স্কোর 

<output value=" /session_18_chap_2/transss "/>/৩</label>
    </input>
    <input ref="/session_18_chap_2/corrAns4">
      <label>আপনার স্কোর 

<output value=" /session_18_chap_2/transsss "/>/৩</label>
    </input>
  </h:body>
</h:html>
