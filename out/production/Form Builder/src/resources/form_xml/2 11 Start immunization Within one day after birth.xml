<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>2.11 Start immunization Within one day after birth</h:title>
    <model>
      <instance>
        <session_11_chap_2_en id="eleven_session_second_chapter_en">
          <formhub>
            <uuid/>
          </formhub>
          <chapter1/>
          <s11_q1/>
          <cal1/>
          <s11_q2/>
          <cal2/>
          <s11_q3/>
          <cal3/>
          <correctAns/>
          <cor1/>
          <cor2/>
          <cor3/>
          <cor0/>
          <meta>
            <instanceID/>
          </meta>
        </session_11_chap_2_en>
      </instance>
      <bind nodeset="/session_11_chap_2_en/chapter1" type="select1"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_11_chap_2_en/s11_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_11_chap_2_en/s11_q1  = 't',1,0)" nodeset="/session_11_chap_2_en/cal1" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_11_chap_2_en/s11_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_11_chap_2_en/s11_q2  = 't',1,0)" nodeset="/session_11_chap_2_en/cal2" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_11_chap_2_en/s11_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_11_chap_2_en/s11_q3  = 't',1,0)" nodeset="/session_11_chap_2_en/cal3" type="string"/>
      <bind calculate=" /session_11_chap_2_en/cal1 + /session_11_chap_2_en/cal2 + /session_11_chap_2_en/cal3 " nodeset="/session_11_chap_2_en/correctAns" type="string"/>
      <bind nodeset="/session_11_chap_2_en/cor1" readonly="true()" relevant=" /session_11_chap_2_en/correctAns  = 1" type="string"/>
      <bind nodeset="/session_11_chap_2_en/cor2" readonly="true()" relevant=" /session_11_chap_2_en/correctAns  = 2" type="string"/>
      <bind nodeset="/session_11_chap_2_en/cor3" readonly="true()" relevant=" /session_11_chap_2_en/correctAns  = 3" type="string"/>
      <bind nodeset="/session_11_chap_2_en/cor0" readonly="true()" relevant=" /session_11_chap_2_en/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_11_chap_2_en/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'fd69f9e618f64115afd84adfbef9cddc'" nodeset="/session_11_chap_2_en/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 appearance="html" ref="/session_11_chap_2_en/chapter1">
      <label>Chapter 2</label>
      <item>
        <label>session_eleven.html</label>
        <value>h</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_11_chap_2_en/s11_q1">
      <label>1. Which vaccines should be given soon after birth?</label>
      <item>
        <label>PCV</label>
        <value>f</value>
      </item>
      <item>
        <label>MR</label>
        <value>ff</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_11_chap_2_en/s11_q2">
      <label>2. What should you apply to BCG scar?</label>
      <item>
        <label>Oil</label>
        <value>t</value>
      </item>
      <item>
        <label>Medicine</label>
        <value>ff</value>
      </item>
      <item>
        <label>Nothing</label>
        <value>f</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_11_chap_2_en/s11_q3">
      <label>3. Where do you dispose sharps?</label>
      <item>
        <label>Safety box</label>
        <value>t</value>
      </item>
      <item>
        <label>Waste paper basket</label>
        <value>f</value>
      </item>
      <item>
        <label>None of above</label>
        <value>ff</value>
      </item>
    </select1>
    <input ref="/session_11_chap_2_en/cor1">
      <label>Your Score 
<output value=" /session_11_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_11_chap_2_en/cor2">
      <label>Your Score 
<output value=" /session_11_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_11_chap_2_en/cor3">
      <label>Your Score 
<output value=" /session_11_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_11_chap_2_en/cor0">
      <label>Your Score 
<output value=" /session_11_chap_2_en/correctAns "/>/3</label>
    </input>
  </h:body>
</h:html>
