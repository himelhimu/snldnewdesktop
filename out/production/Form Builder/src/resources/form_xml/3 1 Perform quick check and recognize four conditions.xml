<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>3.1 Perform “quick check” and recognize four conditions</h:title>
    <model>
      <instance>
        <session_1_chap_3_en id="first_session_third_chapter_en">
          <formhub>
            <uuid/>
          </formhub>
          <chapter1/>
          <s1_q1/>
          <cal1/>
          <s1_q2/>
          <cal2/>
          <s1_q3/>
          <cal3/>
          <correctAns/>
          <cor1/>
          <cor2/>
          <cor3/>
          <cor0/>
          <meta>
            <instanceID/>
          </meta>
        </session_1_chap_3_en>
      </instance>
      <bind nodeset="/session_1_chap_3_en/chapter1" type="select1"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_1_chap_3_en/s1_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_1_chap_3_en/s1_q1  = 't',1,0)" nodeset="/session_1_chap_3_en/cal1" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_1_chap_3_en/s1_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_1_chap_3_en/s1_q2  = 't',1,0)" nodeset="/session_1_chap_3_en/cal2" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_1_chap_3_en/s1_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_1_chap_3_en/s1_q3  = 't',1,0)" nodeset="/session_1_chap_3_en/cal3" type="string"/>
      <bind calculate=" /session_1_chap_3_en/cal1 + /session_1_chap_3_en/cal2 + /session_1_chap_3_en/cal3 " nodeset="/session_1_chap_3_en/correctAns" type="string"/>
      <bind nodeset="/session_1_chap_3_en/cor1" readonly="true()" relevant=" /session_1_chap_3_en/correctAns  = 1" type="string"/>
      <bind nodeset="/session_1_chap_3_en/cor2" readonly="true()" relevant=" /session_1_chap_3_en/correctAns  = 2" type="string"/>
      <bind nodeset="/session_1_chap_3_en/cor3" readonly="true()" relevant=" /session_1_chap_3_en/correctAns  = 3" type="string"/>
      <bind nodeset="/session_1_chap_3_en/cor0" readonly="true()" relevant=" /session_1_chap_3_en/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_1_chap_3_en/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'7e6750e7977e490189efebd7229fce56'" nodeset="/session_1_chap_3_en/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 appearance="html" ref="/session_1_chap_3_en/chapter1">
      <label>Chapter 1</label>
      <item>
        <label>session_one.html</label>
        <value>h</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_1_chap_3_en/s1_q1">
      <label>1. Only women with the following conditions should receive ACS</label>
      <item>
        <label>Preterm labour, severe pre-eclampsia and pPROM</label>
        <value>f</value>
      </item>
      <item>
        <label>Any woman with an increased risk of delivering preterm in the next 7 days</label>
        <value>ff</value>
      </item>
      <item>
        <label>All of above</label>
        <value>t</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_1_chap_3_en/s1_q2">
      <label>2. Which statement is correct for administration of ACS in facility?</label>
      <item>
        <label>Ability to accurately assess GA and determine risk preterm birth only</label>
        <value>f</value>
      </item>
      <item>
        <label>Ability to accurately assess GA and determine risk preterm birth,  adequate care available for preterm newborns reliable, timely and appropriate identification and treatment of maternal infection</label>
        <value>t</value>
      </item>
      <item>
        <label>Timely and appropriate identification and treatment of maternal infection only</label>
        <value>ff</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_1_chap_3_en/s1_q3">
      <label>3. When do women get ACS?</label>
      <item>
        <label>Within gestational age 24-34 week</label>
        <value>t</value>
      </item>
      <item>
        <label>At any time during pregnancy</label>
        <value>f</value>
      </item>
      <item>
        <label>At any time after gestational age of 37 weeks</label>
        <value>ff</value>
      </item>
    </select1>
    <input ref="/session_1_chap_3_en/cor1">
      <label>Your Score 
<output value=" /session_1_chap_3_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_1_chap_3_en/cor2">
      <label>Your Score 
<output value=" /session_1_chap_3_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_1_chap_3_en/cor3">
      <label>Your Score 
<output value=" /session_1_chap_3_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_1_chap_3_en/cor0">
      <label>Your Score 
<output value=" /session_1_chap_3_en/correctAns "/>/3</label>
    </input>
  </h:body>
</h:html>
