<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>২.১ ত্বকে-ত্বক-স্পর্শ পরিচর্যা দিন এবং শ্বাস প্রশ্বাস লক্ষ্য রাখুন</h:title>
    <model>
      <instance>
        <session_1_chap_2 id="first_session_second_chapter">
          <formhub>
            <uuid/>
          </formhub>
          <chapter1/>
          <s1_q1/>
          <cal1/>
          <s1_q2/>
          <cal2/>
          <s1_q3/>
          <cal3/>
          <correctAns/>
          <trans/>
          <transs/>
          <transss/>
          <transsss/>
          <corrAns1/>
          <corrAns2/>
          <corrAns3/>
          <corrAns4/>
          <meta>
            <instanceID/>
          </meta>
        </session_1_chap_2>
      </instance>
      <bind nodeset="/session_1_chap_2/chapter1" type="select1"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_1_chap_2/s1_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_1_chap_2/s1_q1  = 't',1,0)" nodeset="/session_1_chap_2/cal1" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_1_chap_2/s1_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_1_chap_2/s1_q2  = 't',1,0)" nodeset="/session_1_chap_2/cal2" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_1_chap_2/s1_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_1_chap_2/s1_q3  = 't',1,0)" nodeset="/session_1_chap_2/cal3" type="string"/>
      <bind calculate=" /session_1_chap_2/cal1 + /session_1_chap_2/cal2 + /session_1_chap_2/cal3 " nodeset="/session_1_chap_2/correctAns" type="string"/>
      <bind calculate="if( /session_1_chap_2/cal1 + /session_1_chap_2/cal2 + /session_1_chap_2/cal3 =1,'১',' ')" nodeset="/session_1_chap_2/trans" type="string"/>
      <bind calculate="if( /session_1_chap_2/cal1 + /session_1_chap_2/cal2 + /session_1_chap_2/cal3 =2,'২',' ')" nodeset="/session_1_chap_2/transs" type="string"/>
      <bind calculate="if( /session_1_chap_2/cal1 + /session_1_chap_2/cal2 + /session_1_chap_2/cal3 =3,'৩',' ')" nodeset="/session_1_chap_2/transss" type="string"/>
      <bind calculate="if( /session_1_chap_2/cal1 + /session_1_chap_2/cal2 + /session_1_chap_2/cal3 =0,'০',' ')" nodeset="/session_1_chap_2/transsss" type="string"/>
      <bind nodeset="/session_1_chap_2/corrAns1" readonly="true()" relevant=" /session_1_chap_2/correctAns  = 1" type="string"/>
      <bind nodeset="/session_1_chap_2/corrAns2" readonly="true()" relevant=" /session_1_chap_2/correctAns  = 2" type="string"/>
      <bind nodeset="/session_1_chap_2/corrAns3" readonly="true()" relevant=" /session_1_chap_2/correctAns  = 3" type="string"/>
      <bind nodeset="/session_1_chap_2/corrAns4" readonly="true()" relevant=" /session_1_chap_2/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_1_chap_2/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'c5e9e05475f34378900f0ab3566c689a'" nodeset="/session_1_chap_2/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 appearance="html" ref="/session_1_chap_2/chapter1">
      <label>Chapter 2</label>
      <item>
        <label>session_one.html</label>
        <value>h</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_1_chap_2/s1_q1">
      <label>১. স্কিন-টু-স্কিন কেয়ার (ত্বকে-ত্বক-স্পর্শ পরিচর্যা) কখন থেকে শুরু করা উচিৎ?</label>
      <item>
        <label>গর্ভফুল বের হওয়ার পর</label>
        <value>f</value>
      </item>
      <item>
        <label>জন্মের পর শিশুকে মুছে শুষ্ক করার পর</label>
        <value>t</value>
      </item>
      <item>
        <label>আত্মীয়-স্বজনজনকে দেখানোর পর</label>
        <value>ff</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_1_chap_2/s1_q2">
      <label>২. জন্মের প্রথম ঘন্টায় কতক্ষণ পর পর শিশুর শ্বাস-প্রশ্বাস পর্যবেক্ষণ করা উচিৎ?</label>
      <item>
        <label>একবার</label>
        <value>f</value>
      </item>
      <item>
        <label>৩০ মিনিট পরপর</label>
        <value>ff</value>
      </item>
      <item>
        <label>১৫ মিনিট পরপর</label>
        <value>t</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_1_chap_2/s1_q3">
      <label>৩. জন্মের প্রথম ২ ঘন্টায় কি পরিস্থিতিতে স্কিন-টু-স্কিন কেয়ার (ত্বকে-ত্বক-স্পর্শ পরিচর্যা) বন্ধ করা যেতে পারে?</label>
      <item>
        <label>শ্বাস-প্রশ্বাস পর্যবেক্ষণ করা বা অন্য কোন অত্যাবশ্যকীয় সেবা দেয়ার জন্যে</label>
        <value>t</value>
      </item>
      <item>
        <label>আত্মীয়-স্বজনকে দেখানোর জন্যে</label>
        <value>f</value>
      </item>
      <item>
        <label>শিশুকে গোসল করানোর জন্যে</label>
        <value>ff</value>
      </item>
    </select1>
    <input ref="/session_1_chap_2/corrAns1">
      <label>আপনার স্কোর 

<output value=" /session_1_chap_2/trans "/>/৩</label>
    </input>
    <input ref="/session_1_chap_2/corrAns2">
      <label>আপনার স্কোর 

<output value=" /session_1_chap_2/transs "/>/৩</label>
    </input>
    <input ref="/session_1_chap_2/corrAns3">
      <label>আপনার স্কোর 

<output value=" /session_1_chap_2/transss "/>/৩</label>
    </input>
    <input ref="/session_1_chap_2/corrAns4">
      <label>আপনার স্কোর 

<output value=" /session_1_chap_2/transsss "/>/৩</label>
    </input>
  </h:body>
</h:html>
