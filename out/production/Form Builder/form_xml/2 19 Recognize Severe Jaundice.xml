<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>2.19 Recognize Severe Jaundice</h:title>
    <model>
      <instance>
        <session_19_chap_2_en id="nineteen_session_second_chapter_en">
          <formhub>
            <uuid/>
          </formhub>
          <chapter1/>
          <s19_q1/>
          <cal1/>
          <s19_q2/>
          <cal2/>
          <s19_q3/>
          <cal3/>
          <correctAns/>
          <cor1/>
          <cor2/>
          <cor3/>
          <cor0/>
          <meta>
            <instanceID/>
          </meta>
        </session_19_chap_2_en>
      </instance>
      <bind nodeset="/session_19_chap_2_en/chapter1" type="select1"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_19_chap_2_en/s19_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_19_chap_2_en/s19_q1  = 't',1,0)" nodeset="/session_19_chap_2_en/cal1" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_19_chap_2_en/s19_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_19_chap_2_en/s19_q2  = 't',1,0)" nodeset="/session_19_chap_2_en/cal2" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_19_chap_2_en/s19_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_19_chap_2_en/s19_q3  = 't',1,0)" nodeset="/session_19_chap_2_en/cal3" type="string"/>
      <bind calculate=" /session_19_chap_2_en/cal1 + /session_19_chap_2_en/cal2 + /session_19_chap_2_en/cal3 " nodeset="/session_19_chap_2_en/correctAns" type="string"/>
      <bind nodeset="/session_19_chap_2_en/cor1" readonly="true()" relevant=" /session_19_chap_2_en/correctAns  = 1" type="string"/>
      <bind nodeset="/session_19_chap_2_en/cor2" readonly="true()" relevant=" /session_19_chap_2_en/correctAns  = 2" type="string"/>
      <bind nodeset="/session_19_chap_2_en/cor3" readonly="true()" relevant=" /session_19_chap_2_en/correctAns  = 3" type="string"/>
      <bind nodeset="/session_19_chap_2_en/cor0" readonly="true()" relevant=" /session_19_chap_2_en/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_19_chap_2_en/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'b587f87fe04d484bb49f546656a5681b'" nodeset="/session_19_chap_2_en/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 appearance="html" ref="/session_19_chap_2_en/chapter1">
      <label>Chapter 2</label>
      <item>
        <label>session_nineteen.html</label>
        <value>h</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_19_chap_2_en/s19_q1">
      <label>1. When antibiotics should be given to a baby?</label>
      <item>
        <label>If parents desire</label>
        <value>ff</value>
      </item>
      <item>
        <label>If the baby appears to be in pain</label>
        <value>f</value>
      </item>
      <item>
        <label>When a danger sign is present</label>
        <value>t</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_19_chap_2_en/s19_q2">
      <label>2. When should you give the first dose of antibiotic?</label>
      <item>
        <label>As soon as possible after a danger sign has been identified</label>
        <value>t</value>
      </item>
      <item>
        <label>After arrangements have been made for transfer for advanced care</label>
        <value>f</value>
      </item>
      <item>
        <label>If parents desire</label>
        <value>ff</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_19_chap_2_en/s19_q3">
      <label>3. What does determine the dosage of antibiotics?</label>
      <item>
        <label>Time after birth</label>
        <value>f</value>
      </item>
      <item>
        <label>Body weight</label>
        <value>t</value>
      </item>
      <item>
        <label>Size of the available syringe</label>
        <value>ff</value>
      </item>
    </select1>
    <input ref="/session_19_chap_2_en/cor1">
      <label>Your Score 
<output value=" /session_19_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_19_chap_2_en/cor2">
      <label>Your Score 
<output value=" /session_19_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_19_chap_2_en/cor3">
      <label>Your Score 
<output value=" /session_19_chap_2_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_19_chap_2_en/cor0">
      <label>Your Score 
<output value=" /session_19_chap_2_en/correctAns "/>/3</label>
    </input>
  </h:body>
</h:html>
