<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>৩.১ শিশুর সংক্রমণ দ্রুত সনাক্ত করে যথাযথ শ্রেণীবিন্যাস করুন</h:title>
    <model>
      <instance>
        <session_1_chap_3 id="first_session_third_chapter">
          <formhub>
            <uuid/>
          </formhub>
          <chapter3/>
          <s1_q1/>
          <cal1/>
          <s1_q2/>
          <cal2/>
          <s1_q3/>
          <cal3/>
          <correctAns/>
          <trans/>
          <transs/>
          <transss/>
          <transsss/>
          <corrAns1/>
          <corrAns2/>
          <corrAns3/>
          <corrAns4/>
          <meta>
            <instanceID/>
          </meta>
        </session_1_chap_3>
      </instance>
      <bind nodeset="/session_1_chap_3/chapter3" type="select1"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_1_chap_3/s1_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_1_chap_3/s1_q1  = 't',1,0)" nodeset="/session_1_chap_3/cal1" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_1_chap_3/s1_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_1_chap_3/s1_q2  = 't',1,0)" nodeset="/session_1_chap_3/cal2" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_1_chap_3/s1_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_1_chap_3/s1_q3  = 't',1,0)" nodeset="/session_1_chap_3/cal3" type="string"/>
      <bind calculate=" /session_1_chap_3/cal1 + /session_1_chap_3/cal2 + /session_1_chap_3/cal3 " nodeset="/session_1_chap_3/correctAns" type="string"/>
      <bind calculate="if( /session_1_chap_3/cal1 + /session_1_chap_3/cal2 + /session_1_chap_3/cal3 =1,'১',' ')" nodeset="/session_1_chap_3/trans" type="string"/>
      <bind calculate="if( /session_1_chap_3/cal1 + /session_1_chap_3/cal2 + /session_1_chap_3/cal3 =2,'২',' ')" nodeset="/session_1_chap_3/transs" type="string"/>
      <bind calculate="if( /session_1_chap_3/cal1 + /session_1_chap_3/cal2 + /session_1_chap_3/cal3 =3,'৩',' ')" nodeset="/session_1_chap_3/transss" type="string"/>
      <bind calculate="if( /session_1_chap_3/cal1 + /session_1_chap_3/cal2 + /session_1_chap_3/cal3 =0,'০',' ')" nodeset="/session_1_chap_3/transsss" type="string"/>
      <bind nodeset="/session_1_chap_3/corrAns1" readonly="true()" relevant=" /session_1_chap_3/correctAns  = 1" type="string"/>
      <bind nodeset="/session_1_chap_3/corrAns2" readonly="true()" relevant=" /session_1_chap_3/correctAns  = 2" type="string"/>
      <bind nodeset="/session_1_chap_3/corrAns3" readonly="true()" relevant=" /session_1_chap_3/correctAns  = 3" type="string"/>
      <bind nodeset="/session_1_chap_3/corrAns4" readonly="true()" relevant=" /session_1_chap_3/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_1_chap_3/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'48d20f7b0e9f421bac90dced41be68a8'" nodeset="/session_1_chap_3/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 appearance="html" ref="/session_1_chap_3/chapter3">
      <label>Chapter 3</label>
      <item>
        <label>session_one.html</label>
        <value>h</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_1_chap_3/s1_q1">
      <label>১.কোনটি সংকটাপন্ন অসুস্থতার লক্ষণ নয়?</label>
      <item>
        <label>অচেতন / খিঁচুনী / খিঁচুনীর ইতিহাস আছে</label>
        <value>f</value>
      </item>
      <item>
        <label>একদম খেতে না পারা, অনবরত বমি</label>
        <value>ff</value>
      </item>
      <item>
        <label>নাভীর চারপাশ লাল হওয়া</label>
        <value>t</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_1_chap_3/s1_q2">
      <label>২.স্থানীয় সীমিত সংক্রমণ কোনটি?</label>
      <item>
        <label>নাভীর চারপাশ লাল হওয়া</label>
        <value>f</value>
      </item>
      <item>
        <label>ত্বকে পুঁজবটি থাকা</label>
        <value>ff</value>
      </item>
      <item>
        <label>উপরের সব ক’টি</label>
        <value>t</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_1_chap_3/s1_q3">
      <label>৩.তাপমাত্রা কত হলে নিম্ন তাপমাত্রা চিহ্নিত করবেন?</label>
      <item>
        <label>৩৬০সে.</label>
        <value>f</value>
      </item>
      <item>
        <label>&lt;৩৫.৫০সে.</label>
        <value>t</value>
      </item>
      <item>
        <label>৩৮০সে.</label>
        <value>ff</value>
      </item>
    </select1>
    <input ref="/session_1_chap_3/corrAns1">
      <label>আপনার স্কোর 

<output value=" /session_1_chap_3/trans "/>/৩</label>
    </input>
    <input ref="/session_1_chap_3/corrAns2">
      <label>আপনার স্কোর 

<output value=" /session_1_chap_3/transs "/>/৩</label>
    </input>
    <input ref="/session_1_chap_3/corrAns3">
      <label>আপনার স্কোর 

<output value=" /session_1_chap_3/transss "/>/৩</label>
    </input>
    <input ref="/session_1_chap_3/corrAns4">
      <label>আপনার স্কোর 

<output value=" /session_1_chap_3/transsss "/>/৩</label>
    </input>
  </h:body>
</h:html>
