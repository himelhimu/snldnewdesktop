<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>২.১০ শিশুর সঠিক তাপমাত্রা নিয়ন্ত্রণে যথাযথ ব্যবস্থা নিন</h:title>
    <model>
      <instance>
        <session_10_chap_2 id="ten_session_second_chapter">
          <formhub>
            <uuid/>
          </formhub>
          <chapter2/>
          <s10_q1/>
          <cal1/>
          <s10_q2/>
          <cal2/>
          <s10_q3/>
          <cal3/>
          <correctAns/>
          <trans/>
          <transs/>
          <transss/>
          <transsss/>
          <corrAns1/>
          <corrAns2/>
          <corrAns3/>
          <corrAns4/>
          <meta>
            <instanceID/>
          </meta>
        </session_10_chap_2>
      </instance>
      <bind nodeset="/session_10_chap_2/chapter2" type="select1"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_10_chap_2/s10_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_10_chap_2/s10_q1  = 't',1,0)" nodeset="/session_10_chap_2/cal1" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_10_chap_2/s10_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_10_chap_2/s10_q2  = 't',1,0)" nodeset="/session_10_chap_2/cal2" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_10_chap_2/s10_q3" type="select1"/>
      <bind calculate="if( /session_10_chap_2/s10_q3  = 't',1,0)" nodeset="/session_10_chap_2/cal3" type="string"/>
      <bind calculate=" /session_10_chap_2/cal1 + /session_10_chap_2/cal2 + /session_10_chap_2/cal3 " nodeset="/session_10_chap_2/correctAns" type="string"/>
      <bind calculate="if( /session_10_chap_2/cal1 + /session_10_chap_2/cal2 + /session_10_chap_2/cal3 =1,'১',' ')" nodeset="/session_10_chap_2/trans" type="string"/>
      <bind calculate="if( /session_10_chap_2/cal1 + /session_10_chap_2/cal2 + /session_10_chap_2/cal3 =2,'২',' ')" nodeset="/session_10_chap_2/transs" type="string"/>
      <bind calculate="if( /session_10_chap_2/cal1 + /session_10_chap_2/cal2 + /session_10_chap_2/cal3 =3,'৩',' ')" nodeset="/session_10_chap_2/transss" type="string"/>
      <bind calculate="if( /session_10_chap_2/cal1 + /session_10_chap_2/cal2 + /session_10_chap_2/cal3 =0,'০',' ')" nodeset="/session_10_chap_2/transsss" type="string"/>
      <bind nodeset="/session_10_chap_2/corrAns1" readonly="true()" relevant=" /session_10_chap_2/correctAns  = 1" type="string"/>
      <bind nodeset="/session_10_chap_2/corrAns2" readonly="true()" relevant=" /session_10_chap_2/correctAns  = 2" type="string"/>
      <bind nodeset="/session_10_chap_2/corrAns3" readonly="true()" relevant=" /session_10_chap_2/correctAns  = 3" type="string"/>
      <bind nodeset="/session_10_chap_2/corrAns4" readonly="true()" relevant=" /session_10_chap_2/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_10_chap_2/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'c0e8165579f447fbb66e83376f727aeb'" nodeset="/session_10_chap_2/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 appearance="html" ref="/session_10_chap_2/chapter2">
      <label>chapter2</label>
      <item>
        <label>session_ten.html</label>
        <value>h</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_10_chap_2/s10_q1">
      <label>১. জন্মের পর শিশু কখন থেকে তাপ হারাতে থাকে?</label>
