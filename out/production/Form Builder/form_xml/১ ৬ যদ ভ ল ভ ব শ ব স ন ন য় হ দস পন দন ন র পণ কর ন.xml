<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>১.৬ যদি ভালোভাব শ্বাস না নেয় হৃদস্পন্দন নিরুপণ করুন</h:title>
    <model>
      <instance>
        <session_6_chap_1 id="six_session_first_chapter">
          <formhub>
            <uuid/>
          </formhub>
          <chapter1/>
          <c1_s6_q1/>
          <cal1/>
          <c1_s6_q2/>
          <cal2/>
          <c1_s6_q3/>
          <cal3/>
          <correctAns/>
          <trans0/>
          <trans1/>
          <trans2/>
          <trans3/>
          <corrAns0/>
          <corrAns1/>
          <corrAns2/>
          <corrAns3/>
          <meta>
            <instanceID/>
          </meta>
        </session_6_chap_1>
      </instance>
      <bind nodeset="/session_6_chap_1/chapter1" type="select1"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_6_chap_1/c1_s6_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_6_chap_1/c1_s6_q1  = 't',1,0)" nodeset="/session_6_chap_1/cal1" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_6_chap_1/c1_s6_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_6_chap_1/c1_s6_q2  = 't',1,0)" nodeset="/session_6_chap_1/cal2" type="string"/>
      <bind constraint=". = 't'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_6_chap_1/c1_s6_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_6_chap_1/c1_s6_q3  = 't',1,0)" nodeset="/session_6_chap_1/cal3" type="string"/>
      <bind calculate=" /session_6_chap_1/cal1 + /session_6_chap_1/cal2 + /session_6_chap_1/cal3 " nodeset="/session_6_chap_1/correctAns" type="string"/>
      <bind calculate="if( /session_6_chap_1/cal1 + /session_6_chap_1/cal2 + /session_6_chap_1/cal3 =0,'০',' ')" nodeset="/session_6_chap_1/trans0" type="string"/>
      <bind calculate="if( /session_6_chap_1/cal1 + /session_6_chap_1/cal2 + /session_6_chap_1/cal3 =1,'১',' ')" nodeset="/session_6_chap_1/trans1" type="string"/>
      <bind calculate="if( /session_6_chap_1/cal1 + /session_6_chap_1/cal2 + /session_6_chap_1/cal3 =2,'২',' ')" nodeset="/session_6_chap_1/trans2" type="string"/>
      <bind calculate="if( /session_6_chap_1/cal1 + /session_6_chap_1/cal2 + /session_6_chap_1/cal3 =3,'৩',' ')" nodeset="/session_6_chap_1/trans3" type="string"/>
      <bind nodeset="/session_6_chap_1/corrAns0" readonly="true()" relevant=" /session_6_chap_1/correctAns  = 0" type="string"/>
      <bind nodeset="/session_6_chap_1/corrAns1" readonly="true()" relevant=" /session_6_chap_1/correctAns  = 1" type="string"/>
      <bind nodeset="/session_6_chap_1/corrAns2" readonly="true()" relevant=" /session_6_chap_1/correctAns  = 2" type="string"/>
      <bind nodeset="/session_6_chap_1/corrAns3" readonly="true()" relevant=" /session_6_chap_1/correctAns  = 3" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_6_chap_1/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'257d75b5e2d64d7695acf7163fab25cb'" nodeset="/session_6_chap_1/formhub/uuid" type="string"/>
    </model>
  </h:head>
