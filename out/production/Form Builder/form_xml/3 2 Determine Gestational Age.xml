<?xml version="1.0" encoding="utf-8"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>3.2 Determine Gestational Age</h:title>
    <model>
      <instance>
        <session_2_chap_3_en id="second_session_third_chapter_en">
          <formhub>
            <uuid/>
          </formhub>
          <chapter1/>
          <s2_q1/>
          <cal1/>
          <s2_q2/>
          <cal2/>
          <s2_q3/>
          <cal3/>
          <correctAns/>
          <cor1/>
          <cor2/>
          <cor3/>
          <cor0/>
          <meta>
            <instanceID/>
          </meta>
        </session_2_chap_3_en>
      </instance>
      <bind nodeset="/session_2_chap_3_en/chapter1" type="select1"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_2_chap_3_en/s2_q1" required="true()" type="select1"/>
      <bind calculate="if( /session_2_chap_3_en/s2_q1  = 't',1,0)" nodeset="/session_2_chap_3_en/cal1" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_2_chap_3_en/s2_q2" required="true()" type="select1"/>
      <bind calculate="if( /session_2_chap_3_en/s2_q2  = 't',1,0)" nodeset="/session_2_chap_3_en/cal2" type="string"/>
      <bind constraint=".='t'" jr:constraintMsg="উত্তর ভুল" nodeset="/session_2_chap_3_en/s2_q3" required="true()" type="select1"/>
      <bind calculate="if( /session_2_chap_3_en/s2_q3  = 't',1,0)" nodeset="/session_2_chap_3_en/cal3" type="string"/>
      <bind calculate=" /session_2_chap_3_en/cal1 + /session_2_chap_3_en/cal2 + /session_2_chap_3_en/cal3 " nodeset="/session_2_chap_3_en/correctAns" type="string"/>
      <bind nodeset="/session_2_chap_3_en/cor1" readonly="true()" relevant=" /session_2_chap_3_en/correctAns  = 1" type="string"/>
      <bind nodeset="/session_2_chap_3_en/cor2" readonly="true()" relevant=" /session_2_chap_3_en/correctAns  = 2" type="string"/>
      <bind nodeset="/session_2_chap_3_en/cor3" readonly="true()" relevant=" /session_2_chap_3_en/correctAns  = 3" type="string"/>
      <bind nodeset="/session_2_chap_3_en/cor0" readonly="true()" relevant=" /session_2_chap_3_en/correctAns  = 0" type="string"/>
      <bind calculate="concat('uuid:', uuid())" nodeset="/session_2_chap_3_en/meta/instanceID" readonly="true()" type="string"/>
      <bind calculate="'a1232b58b4b545cc80fa09d1de2d8310'" nodeset="/session_2_chap_3_en/formhub/uuid" type="string"/>
    </model>
  </h:head>
  <h:body>
    <select1 appearance="html" ref="/session_2_chap_3_en/chapter1">
      <label>Chapter 2</label>
      <item>
        <label>session_two.html</label>
        <value>h</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_2_chap_3_en/s2_q1">
      <label>1. What are the methods to determine GA?</label>
      <item>
        <label>History of LMP, measuring fundal height</label>
        <value>ff</value>
      </item>
      <item>
        <label>Review or perform USG if necessary and available</label>
        <value>f</value>
      </item>
      <item>
        <label>All of above</label>
        <value>t</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_2_chap_3_en/s2_q2">
      <label>2. Which equation will correctly estimate the expected date of delivery?</label>
      <item>
        <label>(LMP + 9 months) – (7 days) = EDD</label>
        <value>f</value>
      </item>
      <item>
        <label>(LMP + 9 months) + (7 days) = EDD</label>
        <value>t</value>
      </item>
      <item>
        <label>(LMP + 3 months) + (7 days) = EDD</label>
        <value>ff</value>
      </item>
    </select1>
    <select1 appearance="button" ref="/session_2_chap_3_en/s2_q3">
      <label>3. What can be the reason of smaller fundal height than GA?</label>
      <item>
        <label>Fetal under-growth</label>
        <value>t</value>
      </item>
      <item>
        <label>Twin pregnancy</label>
        <value>f</value>
      </item>
      <item>
        <label>Hydramnions</label>
        <value>ff</value>
      </item>
    </select1>
    <input ref="/session_2_chap_3_en/cor1">
      <label>Your Score 
<output value=" /session_2_chap_3_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_2_chap_3_en/cor2">
      <label>Your Score 
<output value=" /session_2_chap_3_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_2_chap_3_en/cor3">
      <label>Your Score 
<output value=" /session_2_chap_3_en/correctAns "/>/3</label>
    </input>
    <input ref="/session_2_chap_3_en/cor0">
      <label>Your Score 
<output value=" /session_2_chap_3_en/correctAns "/>/3</label>
    </input>
  </h:body>
</h:html>
